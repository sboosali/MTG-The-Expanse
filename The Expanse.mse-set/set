	match: Freighter toke<atom-param>*s</atom-param>
	reminder:
		{
		 handle_merged_rt(
		  if param1.value=="ns"
		  then "They’re 0/0 colorless Vehicle artifacts"
		  else "It’s a 0/0 colorless Vehicle artifact"
		  , moved: "Freighter tokens are 0/0 colorless Vehicle artifacts")
		} with “ — This token gets +1/+1 for each .” and crew 1.
	rules:
		eg. 
		
		cf. 
	mode: custom
keyword:
	keyword: Find
	match: find <atom-param>number</atom-param>
	reminder:
		{
		 handle_action_rt(to:"find {param1}", "Look")
		} at the top {
		 if param1.value==1
		 then "card of your library. Put it into your hand."
		 else "{english_number(param1)} cards of your library. Put one of them into your hand and the rest on the bottom in a random order."
		}
	rules: ie. 
	mode: action
keyword:
	keyword: Discover-a
	match: cascade into <atom-param>a</atom-param> <atom-param>p</atom-param> card
	reminder:
		{
		 handle_action_rt(to:"cascade into{param1} {param2}", "Exile")
		} cards from the top of your library until you exile { separate_words(spacer: " and/or ", param2) }. Cast it without paying its mana cost or put it into your hand. Put the rest on the bottom in a random order.
	rules: 
	mode: action
keyword:
	keyword: Discover-a
	match: discover <atom-param>a</atom-param> <atom-param>p</atom-param>
	reminder:
		{
		 handle_action_rt(to:"discover such a card", "Exile")
		} cards from the top of your library until you exile one. Cast it without paying its mana cost or put it into your hand. Put the rest on the bottom in a random order.
	rules: 
	mode: action
